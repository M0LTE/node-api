{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "node_api": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security": {
    "UdpAllowedSourceNetworks": [
      "0.0.0.0/0"
    ],
    "ApiRateLimiting": {
      "Enabled": true,
      "PermitLimit": 100,
      "Window": 60
    },
    "UdpRateLimiting": {
      "Enabled": true,
      "MaxPacketsPerSecondPerIp": 10,
      "MaxTotalPacketsPerSecond": 1000
    },
    "CorsAllowedOrigins": [
      "*"
    ]
  }
}
